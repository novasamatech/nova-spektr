{
  "$schema": "https://unpkg.com/knip@5/schema.json",
  "entry": ["src/renderer/**/*", "src/main/**/*", "tests/**/*", "webpack/*", "scripts/*"],
  "project": ["src/renderer/**/*.{js,ts,tsx}"],
  "webpack": {
    "config": [
      "webpack/*.ts"
    ]
  },
  "playwright": true,
  "storybook": true,
  "tailwind": true,
  "typescript": true,
  "prettier": true,
  "jest": true,
  "eslint": true,
  "lint-staged": true
}
